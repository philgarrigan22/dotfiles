[alias]
aa = add --all
br = branch
bra = branch --all
brd = branch -d
brD = branch -D
ci = commit
cia = commit --amend
co = checkout
cob = checkout -b
cod = checkout develop
com = checkout master
f = fetch
l = pull
last = log --graph --show-signature --pretty=short --max-count=5
lure = pull --rebase
p = push
ra = remote add
rv = remote -v
sdiff = !git diff && git submodule foreach 'git diff'
spush = push --recurse-submodules=on-demand
st = status
stal = stash pop
stap = stash push
supdate = submodule update --remote --merge
tag = tag -s
[commit]
gpgsign = true
template = ~/.gitmessage
[core]
editor = codium --wait
[credential]
helper = osxkeychain
[filter "lfs"]
clean = git-lfs clean -- %f
smudge = git-lfs smudge -- %f
process = git-lfs filter-process
required = true
[github]
user = br3ndonland
[gpg]
program = gpg
[push]
default = simple
[tag]
forceSignAnnotated = true
